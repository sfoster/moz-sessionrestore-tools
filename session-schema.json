{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "version": {
      "type": "array",
      "items": [
        {"type": "string"},
        {"type": "integer"}
      ]
    },
    "windows": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/openWindow"
      }
    },
    "_closedWindows": {
      "type": "array",
      "items": {
        "$ref": "#/definitions/closedWindow"
      }
    }
  },
  "required": ["version", "windows"],
  "definitions": {
    "window": {
      "type": "object",
      "properties": {
        "extData": {"type": "object"},
        "sidebar": {"type": "object"},
        "selected": {"type": "integer"},
        "title": {"type": "string"},
        "tabs": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/tab"
          }
        },
        "_closedTabs": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/closedTab"
          }
        },
        "_lastClosedTabGroupCount": {"type": "integer"},
        "chromeFlags": {"type": "integer"},
        "height": {"type": "integer"},
        "screenX": {"type": "integer"},
        "screenY": {"type": "integer"},
        "sizemode": {"type": "string", "enum": ["normal", "maximized", "minimized", "fullscreen"]},
        "sizemodeBeforeMinimized": {"type": "string", "enum": ["normal", "maximized", "minimized", "fullscreen"]},
        "width": {"type": "integer"},
        "zIndex": {"type": "integer"},
        "workspaceID": {
          "type": ["null", "string"]
        }
      },
      "required": ["tabs"]
    },
    "openWindow": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/window"
        }
      ],
      "properties": {
        "busy": {"type": "boolean"},
        "tabs": {},
        "extData": {},
        "sidebar": {},
        "_closedTabs": {},
        "_lastClosedTabGroupCount": {},
        "chromeFlags": {},
        "height": {},
        "screenX": {},
        "screenY": {},
        "selected": {},
        "sizemode": {},
        "sizemodeBeforeMinimized": {},
        "title": {},
        "width": {},
        "zIndex": {},
        "workspaceID": {}
      },
      "required": [],
      "additionalProperties": false
    },
    "closedWindow": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/window"
        }
      ],
      "properties": {
        "closedAt": {"type": "integer"},
        "closedId": {"type": "integer"},
        "extData": {},
        "sidebar": {},
        "tabs": {},
        "_closedTabs": {},
        "_lastClosedTabGroupCount": {},
        "chromeFlags": {},
        "height": {},
        "screenX": {},
        "screenY": {},
        "selected": {},
        "sizemode": {},
        "sizemodeBeforeMinimized": {},
        "title": {},
        "width": {},
        "zIndex": {},
        "workspaceID": {}
      },
      "required": ["closedAt", "closedId"],
      "additionalProperties": false
    },
    "tab": {
      "type": "object",
      "properties": {
        "entries": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/entry"
          }
        },
        "lastAccessed": {"type": "integer"},
        "hidden": {"type": "boolean"},
        "searchMode": {"type": "null"},
        "userContextId": {"type": "integer"},
        "attributes": {"type": "object"},
        "index": {"type": "integer"},
        "requestedIndex": {"type": "integer"},
        "image": {"type": ["null", "string"]},
        "storage": {
          "type": "object",
          "additionalProperties": {
            "type": "object"
          }
        }
      },
      "required": ["entries", "lastAccessed", "hidden", "userContextId", "index"]
    },
    "openTab": {
      "type": "object",
      "allOf": [
        {
          "$ref": "#/definitions/tab"
        }
      ],
      "properties": {
        "entries": {},
        "lastAccessed": {},
        "hidden": {},
        "searchMode": {},
        "userContextId": {},
        "attributes": {},
        "index": {},
        "requestedIndex": {},
        "image": {},
        "storage": {}
      }
    },
    "closedTab": {
      "type": "object",
      "properties": {
        "state": {
          "$ref": "#/definitions/tab"
        },
        "permanentKey": {"type": "object"},
        "title": {"type": "string"},
        "image": {"type": ["null", "string"]},
        "pos": {"type": "integer"},
        "closedAt": {"type": "integer"},
        "closedId": {"type": "integer"},
        "sourceWindowId": {"type": "string"}
      },
      "additionalProperties": false,
      "required": ["state", "title", "pos", "closedAt", "sourceWindowId", "closedId"]
    },
    "entry": {
      "type": "object",
      "properties": {
        "url": {"type": "string"},
        "title": {"type": "string"},
        "persist": {"type": "boolean"},
        "principalToInherit_base64": {"type": "string"},
        "hasUserInteraction": {"type": "boolean"},
        "triggeringPrincipal_base64": {"type": "string"},
        "resultPrincipalURI": {
          "type": ["null", "string"]
        },
        "children": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/entry"
          }
        }
      },
      "required": ["url", "title", "persist", "hasUserInteraction"]
    }
  }
}
